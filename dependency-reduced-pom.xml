<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <artifactId>pom-scijava</artifactId>
    <groupId>org.scijava</groupId>
    <version>28.0.0</version>
    <relativePath>pom.xml</relativePath>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>in.co.dermatologist</groupId>
  <artifactId>dit4ij</artifactId>
  <name>dit4ij</name>
  <version>0.1.8</version>
  <description>Dicoderma Plugin</description>
  <url>https://github.com/dermatologist/dit4ij</url>
  <issueManagement>
    <system>GitHub Issues</system>
    <url>http://github.com/dermatologist/dit4ij/issues</url>
  </issueManagement>
  <ciManagement>
    <system>None</system>
  </ciManagement>
  <inceptionYear>2013</inceptionYear>
  <mailingLists>
    <mailingList>
      <name>Image.sc Forum</name>
      <archive>https://forum.image.sc/tags/imagej</archive>
    </mailingList>
  </mailingLists>
  <developers>
    <developer>
      <id>dermatologist</id>
      <name>Bell Eapen</name>
      <url>https://imagej.net/User:[MY-IMAGEJ-WIKI-ACCOUNT]</url>
    </developer>
  </developers>
  <contributors>
    <contributor>
      <name>None</name>
    </contributor>
  </contributors>
  <licenses>
    <license>
      <name>Unlicense</name>
      <url>https://unlicense.org/</url>
      <distribution>repo</distribution>
    </license>
  </licenses>
  <scm>
    <connection>scm:git:git://github.com/dermatologist/dit4ij</connection>
    <developerConnection>scm:git:git@github.com:dermatologist/dit4ij</developerConnection>
    <url>https://github.com/dermatologist/dit4ij</url>
  </scm>
  <organization>
    <name>Dermatologists Sans Borders</name>
    <url>https://dermatologist.co.in</url>
  </organization>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-enforcer-plugin</artifactId>
        <version>1.4.1</version>
        <configuration>
          <skip>true</skip>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.2.3</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <artifactSet>
                <includes>
                  <include>in.co.dermatologist:dicoderma</include>
                  <include>org.apache.commons:commons-imaging</include>
                  <include>com.fasterxml.jackson.core:jackson-core</include>
                  <include>com.fasterxml.jackson.dataformat:jackson-dataformat-properties</include>
                  <include>com.fasterxml.jackson.core:jackson-annotations</include>
                  <include>com.fasterxml.jackson.core:jackson-databind</include>
                </includes>
              </artifactSet>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <repositories>
    <repository>
      <id>scijava.public</id>
      <url>https://maven.scijava.org/content/groups/public</url>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>net.imagej</groupId>
      <artifactId>imagej</artifactId>
      <version>2.0.0-rc-72</version>
      <scope>compile</scope>
    </dependency>
  </dependencies>
  <properties>
    <license.licenseName>unlicense</license.licenseName>
    <license.projectName>ImageJ software for multidimensional image processing and analysis.</license.projectName>
    <license.copyrightOwners>N/A</license.copyrightOwners>
    <maven.compiler.source>1.8</maven.compiler.source>
    <maven.compiler.target>1.8</maven.compiler.target>
  </properties>
</project>
